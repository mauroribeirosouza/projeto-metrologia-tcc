<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_metrology_equipamento" model="ir.sequence">
        <field name="name">Sequência Equipamentos</field>
        <field name="code">metrology.equipamento</field>
        <field name="prefix">EQP</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_metrology_calibracao" model="ir.sequence">
    <field name="name">Sequência Calibrações</field>
    <field name="code">metrology.calibracao</field>
    <field name="prefix">CAL</field>
    <field name="padding">5</field>
    <field name="number_next">1</field>
    </record>
</odoo>
